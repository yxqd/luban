# -*- python -*-

import merlin.rules, os
from merlin.Context import create
context = create()
context.code.project = 'luban'


tests = []
import glob
seq = open('.sequence').readlines()
for f in seq:
    f = f.strip()
    tests += glob.glob(f)
    continue


rules = []
rules.append(
    merlin.rules.testSuite(name="luban.db", tests = tests)
    )


recurse_entries = [
    ]
